<header>
  <div class="wrapper">
    <div class="row">
      <img src="/assets/images/logo-white.svg" inline/>
      <div class="greedy">
        <p>{{ metas.site }}</p>
        <p>{{ title }}</p>
      </div>
      <nav>
        <ul>
          <li><a href="/metadata/">Metadata Catalogue</a></li>
          <li><a href="/wycc/">West Yorkshire Data</a></li>
          <li><a href="/nycc/">North Yorkshire Data</a></li>
        </ul>
      </nav>
    </div>
  </div>
  {% if region_key %}
  <nav>
    <ul class="wrapper">
      {% for item in nav_menu %}
        {{ comp.nav.item({ item: item, prefix: region_key }) | safe }}
      {% endfor %}
      </ul>
  </nav>
  {% endif %}
</header>
